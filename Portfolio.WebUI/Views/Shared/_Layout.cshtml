
<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="author" content="M_Adnan" />
    <!-- Document Title -->
    <title>Nurkhan Masimzade Resume / CV HTML5 Template</title>

    <!-- StyleSheets -->
    <link rel="stylesheet" href="~/assets/css/ionicons.min.css">
    <link rel="stylesheet" href="~/assets/css/bootstrap/bootstrap.min.css">
    @*<link rel="stylesheet" href="~/assets/css/font-awesome.min.css">*@
    <link rel="stylesheet" href="~/assets/css/main.css">
    <link rel="stylesheet" href="~/assets/css/style.css">
    <link rel="stylesheet" href="~/assets/css/responsive.css">
    @RenderSection("addcss",required:false)
    <!-- FontsOnline -->
    <link href="https://fonts.googleapis.com/css?family=Raleway:100,200,300,400,500,600,700,800,900" rel="stylesheet">
    <script src="https://kit.fontawesome.com//1154e1d6ee.js" crossorigin="anonymous"></script>
    <!-- JavaScripts -->
    <script src="~/assets/js/vendors/modernizr.js"></script>
</head>
<body>

    <!-- Page Wrapper -->
    <div id="wrap">

        <!-- Header -->
        <nav id="cd-lateral-nav">
            <h5 class="tittle">Nurkhan CV</h5>
            <h5 class="tittle">Latest Post</h5>
            <ul class="latest-post">

                <!-- Post Small -->
                <li>
                    <div class="media">
                        <div class="media-left"> <a href="#."><img src="~/assets/images/small-img.jpg" alt=""></a></div>
                        <div class="media-body">
                            <a href="#.">
                                Hello Itaewon
                                Project
                            </a>
                            <p class="font-crimson font-italic">Webdesign</p>
                        </div>
                    </div>
                </li>
                <!-- Post Small -->
                <li>
                    <div class="media">
                        <div class="media-left"> <a href="#."><img src="~/assets/images/small-img-1.jpg" alt=""></a></div>
                        <div class="media-body">
                            <a href="#.">Casual in grey</a>
                            <p class="font-crimson font-italic">Webdesign</p>
                        </div>
                    </div>
                </li>
                <!-- Post Small -->
                <li>
                    <div class="media">
                        <div class="media-left"> <a href="#."><img src="~/assets/images/small-img-2.jpg" alt=""></a></div>
                        <div class="media-body">
                            <a href="#.">Transittions</a>
                            <p class="font-crimson font-italic">Webdesign</p>
                        </div>
                    </div>
                </li>
            </ul>
        </nav>

        <!-- End Header -->
        <!-- Content -->
        <main class="cd-main-content">
            <div id="content" class="bg">
                <div class="resume">
                    <div class="container">

                        <!-- TOP HEAD -->
                        <div class="top-head">
                            <div class="row">
                                <div class="col-sm-6">
                                    <h4>Resume </h4>
                                    <a asp-area="admin" asp-controller="home" asp-action="index">Edit Resume</a>
                                </div>
                                <div class="col-sm-6"> <a id="cd-menu-trigger" href="#0"><span class="cd-menu-icon"></span></a> </div>
                            </div>
                        </div>

                        <!-- Resume -->
                        <div class="row">

                            <!-- Sidebar -->
                            <div class="col-md-4">
                                <div class="side-bar">

                                    <!-- Professional Details -->
                                    <h5 class="tittle">Professional Details</h5>
                                    @await Component.InvokeAsync("ProfessionalDetails")

                                    <!-- Attachments -->
                                    <h5 class="tittle">Attachments</h5>
                                    <div class="attach bor-btm padding-25">
                                        <ul>
                                            <form asp-action="pdf" asp-controller="pdf" method="post">
                                            <li>
                                                <p><img src="~/assets/images/pdf-icon.jpg" alt=""> <button>Curriculum-Vitae.pdf</button> <a href="#."><i class="fa fa-cloud-download"></i></a> <a href="#."><i class="fa fa-eye"></i></a></p>
                                            </li>
                                            </form>
                                        </ul>
                                    </div>

                                    <!-- Social Profiles -->
                                    <h5 class="tittle">Social Profiles</h5>
                                    <div class="social-icon bor-btm padding-25">
                                        <ul>
                                            <li> <a href="#."> <i class="fa fa-facebook"></i></a> </li>
                                            <li> <a href="#."> <i class="fa fa-google"></i></a> </li>
                                            <li> <a href="#."> <i class="fa fa-twitter"></i></a> </li>
                                            <li> <a href="#."> <i class="fa fa-linkedin"></i></a> </li>
                                            <li> <a href="#."> <i class="fa fa-skype"></i></a> </li>
                                        </ul>
                                    </div>

                                    <!-- Contact Me -->
                                    <h5 class="tittle">Contact Me</h5>
                                    <div class="contact padding-25">
                                        <!-- Success Msg -->
                                        <div id="contact_message" class="success-msg"> <i class="fa fa-paper-plane-o"></i>Thank You. Your Message has been Submitted</div>

                                        <!-- FORM -->

                                        <form role="form" id="contact_form" class="contact-form" method="post">
                                            <ul class="row">
                                                <li class="col-sm-12">
                                                    <label>
                                                        <input  class="form-control" id="email" placeholder="Email">
                                                    </label>
                                                </li>
                                                <li class="col-sm-12">
                                                    <label>
                                                        <input  class="form-control" id="company" placeholder="Subject">
                                                    </label>
                                                </li>
                                                <li class="col-sm-12">
                                                    <label>
                                                        <textarea class="form-control"  id="message" rows="5" placeholder="Message"></textarea>
                                                    </label>
                                                </li>
                                                <li class="col-sm-12">
                                                    <button type="submit" value="submit" id="btn_submit">Send Message</button>
                                                </li>
                                            </ul>
                                        </form>
                                    </div>
                                </div>
                            </div>
                            
                            <!-- MAIN CONTENT -->
                            <div class="col-md-8">

                                <!-- NAV LINKS -->
                                <nav>
                                    <!-- Brand and toggle get grouped for better mobile display -->
                                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#nav-tabis" aria-expanded="false"> <span class="tittle">MENU</span> <span class="fa fa-navicon icon-nav"></span> </button>
                                    <!-- Collect the nav links, forms, and other content for toggling -->
                                    <div class="collapse navbar-collapse" id="nav-tabis">
                                        <ul class="isop-filter nav nav-pills">
                                            <li role="presentation" class="active"><a asp-controller="Home" asp-action="Index"><i class="icon-user"></i> ABOUT ME</a></li>
                                            <li role="presentation"><a asp-controller="Home" asp-action="Resume" ><i class="icon-book-open"></i>RESUME</a></li>
                                            <li role="presentation"><a asp-controller="Blog" asp-action="Index"><i class="icon-note"></i>BLOG</a></li>
                                            <li role="presentation"><a asp-controller="Blog" asp-action="Portfolio"><i class="icon-rocket"></i>PORTFOLIO</a></li>
                                            <li role="presentation"><a asp-controller="Home" asp-action="Contact"> <i class="icon-pencil"></i>CONTACT ME</a></li>
                                        </ul>
                                    </div>
                                </nav>
                                <!-- NAV LINKS END -->

                                <div class="tab-content">

                                    @RenderBody()

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </main>
        <!-- End Content -->
        <!-- Footer -->
        <footer class="footer">
            <div class="rights">
                <p>© Copyright 2022 ElgunCV. All right reserved.</p>
            </div>
        </footer>
        <!-- End Footer -->

    </div>
    <!-- End Page Wrapper -->
    <!-- JavaScripts -->
    <script src="~/assets/js/vendors/jquery/jquery.min.js"></script>
    <script src="~/assets/js/vendors/bootstrap.min.js"></script>
    <script src="~/assets/js/vendors/owl.carousel.min.js"></script>
    <script src="~/assets/js/vendors/jquery.isotope.min.js"></script>
    <script src="~/assets/js/main.js"></script>
    @RenderSection("addjs",required:false)

    <!-- Begin Map Script-->
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp"></script>
    <script src="~/assets/js/vendors/map.js"></script>
</body>

</html>